{
  "name": "fluent-downloader",
  "version": "0.0.2",
  "description": "Multi Threaded Downloader",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mosamuhana/fluent-downloader.git"
  },
  "keywords": [
    "Multi Threaded",
    "Multi Downloads",
    "download",
    "Threads",
    "Thread",
    "Worker",
    "Atomics"
  ],
  "author": "Mosa Muhana (https://github.com/mosamuhana)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mosamuhana/fluent-downloader/issues"
  },
  "homepage": "https://github.com/mosamuhana/fluent-downloader#readme",
  "engines": {
    "node": ">=10.5.0"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"bin": {
    "fdl": "./bin/cli.js"
  },
	"cliName": "fdl",
  "scripts": {
    "prebuild": "rimraf -rf ./dist",
    "build": "rollup -c"
  },
  "dependencies": {
    "@devteks/cursor": "^0.0.1",
    "@devteks/node-workers": "^0.0.6",
    "@devteks/progress": "^0.0.5",
    "axios": "^0.27.2",
    "commander": "^9.2.0",
    "mime-db": "^1.52.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-google": "^0.14.0",
    "rollup": "^2.74.1",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dts": "^4.2.2",
    "typescript": "^4.7.2"
  }
}
